{
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"decorators": true,
			"dynamicImport": true,
			"importAssertions": true
		},
		"transform": {},
		"target": "es2016",
		"loose": true,
		"externalHelpers": false,
		"keepClassNames": true,
		"minify": {
			"compress": false,
			"mangle": false
		},
		"paths": {
			"@kernel": ["../core/src/index.js"],
			"@kernel/*": ["../core/src/*"]
		}
	},
	"module": {
		"type": "commonjs",
		"strict": false,
		"strictMode": true,
		"lazy": false,
		"noInterop": false
	},
	"minify": false,
	"sourceMaps": "inline"
}
